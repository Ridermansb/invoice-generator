{
  "name": "Invoice generator",
  "description": "Invoice generator",
  "version": "1.0.1",
  "manifest_version": 3,
  "permissions": ["downloads", "storage"],
  "action": {
    "default_icon": {
      "16": "/images/16.png",
      "32": "/images/32.png",
      "48": "/images/48.png",
      "128": "/images/128.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "/images/16.png",
    "32": "/images/32.png",
    "48": "/images/48.png",
    "128": "/images/128.png"
  },
  "options_page": "options.html"
}
